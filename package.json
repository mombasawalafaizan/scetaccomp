{
  "name": "desktop",
  "version": "0.0.0",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix frontend",
    "server": "nodemon server.js",
    "client": "cd frontend && npm run start",
    "dev": "concurrently --kill-others-on-fail \"nodemon server.js\" \"npm run client\""
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ],
  "start": "nodemon -e js, ejs",
